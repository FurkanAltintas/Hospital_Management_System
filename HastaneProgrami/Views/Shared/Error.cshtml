<!DOCTYPE html>
<html lang="en">
    @Html.Partial("_PartialHead")
<body>
    <!-- Preloader -->
    <div class="preloader">
        <div class="cssload-speeding-wheel"></div>
    </div>
    <section id="wrapper" class="error-page">
        <div class="error-box">
            <div class="error-body text-center">
                <h1>404</h1>
                <h3 class="text-uppercase">Sayfa Bulunamadı !</h3>
                <p class="text-muted m-t-30 m-b-30">Üzgünüz...Aradığınız sayfayı bulamadık.Lütfen Kontrol Ediniz.</p>
                <a href="@Url.Action("","")" class="btn btn-info btn-rounded waves-effect waves-light m-b-40">Geriye Dön</a>  
            </div>
            <footer class="footer text-center"></footer>
        </div>
    </section>
    @Html.Partial("_PartialScripts")
</body>
</html>

