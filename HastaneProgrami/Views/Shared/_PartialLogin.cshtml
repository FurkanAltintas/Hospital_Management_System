<div class="form-group">
    <div class="col-xs-12">
        @ViewBag.LoginError
    </div>
</div>
<div class="form-group">
    <div class="col-md-12">
        <div class="checkbox checkbox-primary pull-left p-t-0">
            <input id="checkbox-signup" type="checkbox">
            <label for="checkbox-signup"> Beni Hatırla </label>
        </div>
        <a href="javascript:void(0)" id="to-recover" class="text-dark pull-right"><i class="fa fa-lock m-r-5"></i> Şifremi Unuttum?</a>
    </div>
</div>
<div class="form-group text-center m-t-20">
    <div class="col-xs-12">
        <button value="Login" class="btn btn-info btn-lg btn-block text-uppercase waves-effect waves-light" type="submit">Giriş Yap</button>
    </div>
</div>
<div class="form-group m-b-0">
    <div class="col-sm-12 text-center">
        <p>Kaydın yok mu? <a href="/Login/UyeOl" class="text-primary m-l-5"><b>Üye Ol</b></a></p>
    </div>
</div>
<br />
<div class="form-group m-b-0">
    <div class="col-sm-12 text-center">
        <a href="/Login/Vatandas" class="text-primary m-l-5"><b>Vatandaş Girişi</b></a>
    </div>
</div>
