@model HastaneProgrami.Models.Entity_Framework.ameliyatEkip

@{
    ViewBag.Title = "AMELİYAT EKİBİ SİLME";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <hr />

        <div class="form-group">
            @Html.LabelFor(model => model.ad, "AMELİYAT EKİBİ ADI")
            <div class="input-group">
                <div class="input-group-addon"><i class="fa fa-user"></i></div>
                @Html.TextBoxFor(model => model.ad, new { @class = "form-control", @readonly = "readonly" })
                @Html.ValidationMessageFor(model => model.ad, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <a href="@Url.Action("Index","AmeliyatEkip")" class="btn btn-facebook">Geriye Dön</a>
            <button class="btn btn-googleplus">AMELİYAT EKİP SİL</button>
        </div>
    </div>
}


